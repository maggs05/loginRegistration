<!DOCTYPE html>
@model loginRegistration.Models.HomePageUsers

<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title>Document</title>
</head>
<body>
    <h1>Login and Register!</h1>
    <hr>
    @Html.Partial("Registration", Model.RegisterUser)
    @Html.Partial("Login", Model.LoginUser)
    @foreach(var user in Model.Users)
    {
        <p>@user["Firstname"]</p>
    }
</body>
</html>
